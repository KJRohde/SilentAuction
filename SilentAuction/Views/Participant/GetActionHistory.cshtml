@model IEnumerable<SilentAuction.Models.ParticipantAction>
@{
    ViewBag.Title = "GetActionHistory";
}

<h2>Items Won</h2>

<table class="table">
    <tr>
        <th>
            Action
        </th>
        <th>
            Time
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Action)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Time)
            </td>
        </tr>
    }
</table>